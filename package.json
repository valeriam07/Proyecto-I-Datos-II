{
 "name": "proyecto-i-datos-ii",
 "version": "1.0.0",
 "description": "En este repositorio se encuentra el proceso de implementacion de una extension de Visual Studio Code para  el manejo de memoria.",
 "main": "src/index.js",
 "gypfile": true,
 "contributes": {
  "commands": [
   {
    "command": "testaddon.start",
    "title": "test addon coding session",
    "category": "testaddon"
   },
   {
    "command": "testaddon.doRefactor",
    "title": "Do some refactoring",
    "category": "testaddon"
   }
  ]
 },
 "engines": {
  "vscode": "^1.38.0"
 },
 "scripts": {
  "test": "echo \"Error: no test specified\" && exit 1",
  "build": "node-gyp rebuild",
  "clean": "node-gyp clean",
  "vscode:prepublish": "npm run compile",
  "compile": "tsc -p ./",
  "lint": "tslint -p ./",
  "watch": "tsc -w -p ./"
 },
 "categories": [
  "Other"
 ],
 "activationEvents": [
  "onCommand:testaddon.start",
  "onCommand:testaddon.doRefactor",
  "onWebviewPanel:testaddon"
 ],
 "repository": {
  "type": "git",
  "url": "git+https://github.com/valeriam07/Proyecto-I-Datos-II.git"
 },
 "author": "",
 "license": "ISC",
 "bugs": {
  "url": "https://github.com/valeriam07/Proyecto-I-Datos-II/issues"
 },
 "homepage": "https://github.com/valeriam07/Proyecto-I-Datos-II#readme",
 "devDependencies": {
  "@types/node": "^12.12.41",
  "@types/vscode": "^1.38.0",
  "node-gyp": "^6.1.0",
  "tslint": "^5.19.0",
  "typescript": "^3.8.3"
 },
 "dependencies": {
  "@types/jquery": "^3.3.38",
  "jsonpath": "^1.0.2",
  "node-addon-api": "^3.0.0",
  "requirejs": "^2.3.6",
  "vscode": "^1.1.37"
 }
}
